<div class="mfr_relation_other">
    <form autocomplete="off"
        hx-post="\\post_prefix///relation\\@index.@exists/index?//:\\@index|index ~//"
        hx-trigger="change from:#relation_other\\@index|index//_input"
        hx-include=".submitted_field"
        hx-target="closest div.sortable_row"
        hx-swap="outerHTML">
        <input type="search" autocomplete="off"
            id="relation_other\\@index|index//_input"
            name="relation_other\\@index|index//"
            list="relation_other\\@index|index//_datalist"
\\err_other?
            class="submitted_field invalid"
            hx-vals='{"invalid":true}'
\\;
            class="submitted_field"
\\~
            value="\\other//"
            placeholder="Manufacturer"
            hx-post="/mfr"
            hx-trigger="input changed delay:200ms"
            hx-params="relation_other\\@index|index//"
            hx-target="#relation_other\\@index|index//_datalist"
            hx-swap="innerHTML"
            hx-sync="closest form:abort">
        <datalist id="relation_other\\@index|index//_datalist"></datalist>
    </form>
</div>
